<SCRIPT type="text/javascript">

function selectTag(showContent,selfObj){
	// 標籤
	var tag = document.getElementById("tags1").getElementsByTagName("li");
	var taglength = tag.length;
	for(i=0; i<taglength; i++){
	tag[i].className = "";
	}
	document.getElementById(""+selfObj).parentNode.className = "selectTag";
	// 標籤內容
	for(i=0; j=document.getElementById("tagContent"+i); i++){
	j.style.display = "none";
	}
	document.getElementById(showContent).style.display = "block";
	setChip($$system_name,"selfObj",selfObj);
}

var disply0 = "";
var disply1 = "";
var disply2 = "";
var disply3 = "";
var chkvar0 = true;
var chkvar1 = true;
var chkvar2 = true;
var chkvar3 = true;
function disply(id,formName,chk){
	var fn = formName;
	if (chkvar0 || chkvar1 || chkvar2){
		if (id == "tagContent0"){
			ajax1(document.index_form,""+id+"");
			disply0 = document.getElementById("tagContent0").innerHTML;
			chkvar0 = chk;
		}
		if (id == "tagContent1"){
			ajax1(document.index_form,""+id+"");
			disply1 = document.getElementById("tagContent1").innerHTML;
			chkvar1 = chk;
		}
		if (id == "tagContent2"){
			ajax1(document.index_form,""+id+"");
			disply2 = document.getElementById("tagContent2").innerHTML;
			chkvar2 = chk;
		}
		if (id == "tagContent3"){
			ajax1(document.index_form,""+id+"");
			disply3 = document.getElementById("tagContent3").innerHTML;
			chkvar3 = chk;
		}
	}else{
		if (id == "tagContent0"){
			document.getElementById("tagContent0").innerHTML = disply0;
		}
		if (id == "tagContent1"){
			document.getElementById("tagContent1").innerHTML = disply1;
		}
		if (id == "tagContent2"){
			document.getElementById("tagContent2").innerHTML = disply2;
		}
		if (id == "tagContent3"){
			document.getElementById("tagContent3").innerHTML = disply3;
		}
	}
}

function parammenu(){
	getMsg();
	showMsg();
	//alert(document.fh.menusave.value);
	/*
	if (document.fh.menusave.value=="1"){
		showIt11();
		chantxt11();

	}else if (document.fh.menusave.value=="2"){
		showIt21();
		chantxt21();

	}else if (document.fh.menusave.value=="3"){
		showIt31();
		chantxt31();

	}else if (document.fh.menusave.value=="4" ){
		showIt41();
		chantxt41();

	}else if (document.fh.menusave.value=="5"){
		showIt51();
		chantxt51();

	}else if (document.fh.menusave.value=="6"){
		showIt61();
		showIt2();

	}else if (document.fh.menusave.value==""){
		document.fh.menusave.value="6";

		showIt61();
		showIt2();

	}
	*/

	$$system_name = "newmops2";

	if (getChip($$system_name,"selfObj")==null){
		selectTag("tagContent1","tagCon1");
		document.index_form.stp.value="1";
		//document.index_form.TYPEK.value="all";
		disply("tagContent1","index_form",false);
	}else{
		v=getChip($$system_name,"selfObj");
		//t=getChip($$system_name,"TYPEK");
		if (v=="tagCon0"){
			selectTag("tagContent0",v);
			document.index_form.stp.value="0";
			//document.index_form.TYPEK.value=t;
			disply("tagContent0","index_form",false);
		}
		if (v=="tagCon1"){
			selectTag("tagContent1",v);
			document.index_form.stp.value="1";
			//document.index_form.TYPEK.value=t;
			disply("tagContent1","index_form",false);
		}
		if (v=="tagCon2"){
			selectTag("tagContent2",v);
			document.index_form.stp.value="2";
			disply("tagContent2","index_form",false);
		}
		if (v=="tagCon3"){
			selectTag("tagContent3",v);
			document.index_form.stp.value="3";
			disply("tagContent3","index_form",false);
		}
	}
}

window.onload=parammenu;
