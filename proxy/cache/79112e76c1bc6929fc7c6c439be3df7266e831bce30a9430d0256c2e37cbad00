<script type="text/javascript">
	function showMsg(){
		var toDate = new Date();
		var year="";
		if (toDate.getYear() < 1999) {
			year = 1900+toDate.getYear();
		}else{
			year = toDate.getYear();
		}
		var month = toDate.getMonth()+1;
		if (month<10){
			month = "0"+month;
		}
		var day = toDate.getDate();
		if (day<10){
			day = "0"+day;
		}
		var hour = toDate.getHours();
		if (hour<10){
			hour = "0"+hour;
		}
		var minute = toDate.getMinutes();
		if (minute<10){
			minute = "0"+minute;
		}
		var second = toDate.getSeconds();
		if (second<10){
			second = "0"+second;
		}
		var toDay = ""+year+""+month+""+day+""+hour+""+minute+""+second;
		//if (toDay >= 20130205100000 && toDay <= 20130217080000){
		if (toDay <= 20171022140000){
			window.open ("/MOPS_Bulletin.html", "MOPSBeta", "height=300, width=750, toolbar=no, menubar=no, //scrollbars=no, resizable=no, location=no, status=no")
		}
	}

</script>
</html>

</html>
